<%- include ('includes/head.ejs') %>

<link rel="stylesheet" href="/CSS/product.css">
</head>

<body>
    <%- include ('includes/navigation.ejs') %>
    <main>
        <% if (prods.length > 0) { %>
        <h1>My Products</h1>
        <p>List of all the products...</p>
        <% for( let product of prods ) { %>
        <h1><%= product.title %> </h1>
        <img src="https://media.istockphoto.com/photos/library-picture-id1028739954?k=6&m=1028739954&s=612x612&w=0&h=rCJgAeeLo3BYYz7Jyr_2D0ywSMmkTCwIEmM7nfXRe4w="
            alt="">
        <% } %>
        <% } else { %>
        <h1>No Products</h1>
        <% } %>
    </main>
    <%- include ('includes/end.ejs') %>